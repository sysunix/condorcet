{
  "name": "condorcet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn build && firebase serve --only hosting -p 3000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "fixtures": "node functions/fixtures/index.js",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "setup": "yarn install && cpy .env.example . --rename=.env",
    "teo": "yarn test:e2e:open",
    "test:e2e": "yarn test:e2e:config && cypress run --env envName=stage",
    "test:e2e:config": "cypress-firebase createTestEnvFile",
    "test:e2e:open": "yarn test:e2e:config && cypress open",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "yarn test:unit --watch",
    "tuw": "yarn test:unit:watch"
  },
  "dependencies": {
    "@firebase/app": "0.4.12",
    "@firebase/auth": "0.11.6",
    "@firebase/firestore": "1.4.7",
    "@sentry/browser": "5.5.0",
    "@sentry/integrations": "5.5.0",
    "cytoscape": "3.8.2",
    "indicative": "6.0.1",
    "random-string": "0.2.0",
    "register-service-worker": "1.6.2",
    "shuffle-array": "1.0.1",
    "slugify": "1.3.4",
    "typeit": "6.0.3",
    "uuid": "3.3.2",
    "vue": "2.6.10",
    "vue-clipboard2": "0.3.0",
    "vue-i18n": "^8.0.0",
    "vue-router": "3.0.7",
    "vue-slicksort": "1.1.3",
    "vue-spinner": "1.0.3",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.9.2",
    "@vue/cli-plugin-eslint": "3.9.2",
    "@vue/cli-plugin-pwa": "3.9.0",
    "@vue/cli-plugin-unit-jest": "3.9.0",
    "@vue/cli-service": "3.9.3",
    "@vue/eslint-config-prettier": "5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "codecov": "3.5.0",
    "cpy-cli": "2.0.0",
    "cypress": "3.4.0",
    "cypress-firebase": "0.2.0",
    "dotenv": "8.0.0",
    "eslint": "6.1.0",
    "eslint-plugin-cypress": "2.6.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "5.2.3",
    "faker": "4.1.0",
    "firebase-mock": "2.2.10",
    "firebase-tools": "7.2.1",
    "jest-when": "2.6.0",
    "lodash": "4.17.15",
    "sort-keys": "3.0.0",
    "tailwindcss": "1.0.5",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10"
  }
}
